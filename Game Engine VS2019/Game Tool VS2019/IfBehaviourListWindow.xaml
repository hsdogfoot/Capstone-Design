<Window x:Class="Game_Tool_VS2019.IfBehaviourListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game_Tool_VS2019"
        mc:Ignorable="d"
        Title="IfBehaviourListWindow"
        
        Width="400" Height="600"
        IsVisibleChanged="OnVisibleChanged_IfBehaviourWindow"
        Closing="OnClosingWindow_IfBehaviourWindow">
    <Grid>
        <ListBox x:Name="IfBehaviourListBox"
                 MouseDoubleClick="OnMouseDoubleClicked_IfBehaviourListBox"
                 Background="AliceBlue">
            
        </ListBox>
    </Grid>
</Window>
