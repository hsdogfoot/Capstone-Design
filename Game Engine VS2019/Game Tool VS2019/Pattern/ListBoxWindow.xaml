<Window x:Class="Game_Tool_VS2019.Pattern.ListBoxWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game_Tool_VS2019.Pattern"
        mc:Ignorable="d"
        
        Title="ListBoxWindow" 
        Width="400" Height="600"
        
        IsVisibleChanged="OnVisibleChanged_ListBoxWindow"
        Closing="OnColsing_ListBoxWindow">
    <Grid>
        <ListBox x:Name="PatternPartsList"
                 Background="AliceBlue"
                 MouseDoubleClick="OnMouseDoubleClick_PatternPartsList">
        </ListBox>
    </Grid>
</Window>
